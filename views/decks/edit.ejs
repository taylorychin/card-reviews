<%- include("../partials/header")%>



    <form id="edit-form" action="/decks/<%= deck._id %>?_method=PUT" method="POST">
        <div>
            <label>Deck Name: </label>
        </div>
        <input type="text" name="name" value="<%= deck.name %>"> <br><br>
        <div>
            <label>Designing Company:</label>
        </div>
        <input type="text" name="company" value="<%= deck.company %>">
        <br><br>
        <div>
            <label>Printing Company:</label>
        </div>
        <input type="text" name="producer" value="<%= deck.producer %>">
        <br><br>
        <div>
            <label>Description:</label>
        </div>
        <textarea name="desc"> <%= deck.desc %>  </textarea>
        <br><br>
        <div>
            <label>picture link</label>
        </div>
        <input type="text" name="picture" value="<%= deck.picture %>">
        <br><br>

        <input type="submit" value="Update Deck">
    </form>


    <%- include("../partials/footer")%>